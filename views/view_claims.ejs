<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/css/util.css">
	<link rel="stylesheet" type="text/css" href="/css/main.css">
<!--===============================================================================================-->
    <style>
        .navbar-brand{
          color: #7AACB3;
      }
        .wrap-contact2{
            width: 800px;
            height: auto;
        }
    </style>
</head>
<body>
    <!-- Navigation -->
  <nav class="navbar bg-dark fixed-top" id="mainNav">
    <div class="container">
      <a class="navbar-brand" href="/"><img src="/logo60.png" />MIS | Insurance Company</a>
      <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
        Menu
      </button>
      <li class="nav-item">
        <a class="nav-link" href="/logout">logout</a>
      </li>
      <div class="collapse navbar-collapse" id="navbarResponsive">
        <ul class="navbar-nav ml-auto">
          <li class="nav-item">
            <a class="nav-link js-scroll-trigger" href="/login/insuranceCompany">View claims</a>
          </li>
          
        </ul>
      </div>
    </div>
  </nav>

	<div class="bg-contact2" style="background-image: url('/images/bg-04.jpg');">
		<div class="container-contact2">
			<div align=center class="wrap-contact2">
				
					<span class="contact2-form-title">
						View claims
					</span>
                    <% for(var i=0;i<results.length;i++){ %>
                                        <div style="display: inline-block; border: 1px solid; padding: 10px; border-radius: 0.5em;">
                                            <span>No | <%= results[i].chart_no %></span><br/>
                                        <span>Date | <%= results[i].i_receipt_time %></span><br/>
                                       <span>Patient | <%= results[i].patient_id%></span><br/>
                                        <span>Treatment | <%= results[i].med_tr_name %></span><br/>
                                        <span>Total expense | <%= results[i].tot_expense %></span><br/><br/>
                                        
                                        </div><br/><br/>
                        <%    } %>
                   
                    

				
			</div>
		</div>
	</div>




<!--===============================================================================================-->
	<script src="/vendor/jquery/jquery-3.2.1.min.js"></script>
